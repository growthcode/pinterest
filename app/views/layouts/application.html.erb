<!DOCTYPE html>
<html>
<head>
  <title>Pinterest by GrowthCode</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>



</head>
<body>

  <div class="container">
    <div class="row">
      <div class="col-md-12">
        <%= render 'layouts/header' %>
      </div>
    </div>
  </div>

  <%= yield %>

  <div class="container">
    <div class="row">
      <div class="col-md-12">
        <%= render 'layouts/footer' %>
      </div>
    </div>
  </div>

</body>
</html>
